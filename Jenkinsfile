pipeline {
  agent any
  stages {
    stage('version check') {
      parallel {
        stage('version check') {
          steps {
            echo 'selam'
            sh 'ansible --version'
          }
        }

        stage('') {
          steps {
            echo 'merhaba'
          }
        }

      }
    }

    stage('write tata') {
      steps {
        echo 'tata'
      }
    }

  }
}